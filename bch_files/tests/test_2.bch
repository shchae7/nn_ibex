function relu(x)
    return max(0,x)
end

variables
x1 in [1,2];
x2 in [1,2];
z1[3];
z2[2];
y[1];

constraints
z1[0] = relu(x1 * 2.0 + x2 * -1.0);
z1[1] = relu(x1 * 1.0 + x2 * 3.0);
z1[2] = relu(x1 * -2.0 + x2 * 1.0);
z2[0] = relu(z1[0] * 1.0 + z1[1] * 2.0 + z1[2] * 3.0);
z2[1] = relu(z1[0] * 2.0 + z1[1] * -1.0 + z1[2] * 2.0);
y[0] = z2[0] * 2.0 + z2[1] * 1.0;
y[0] > 20;

end
